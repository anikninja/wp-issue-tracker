{
  "name": "website-issue-tracker",
  "version": "1.0.0",
  "description": "Issue Tracker For WordPress.",
  "main": "clear && npm run watch",
  "URL": "https://themeoo.com/plugins/website-issue-tracker",
  "scripts": {
    "post-install": "npm rebuild",
    "start": "npm run watch",
    "test": "npm run eslint && npm run stylish",
    "eslint": "eslint . -o ./logs/eslint.log -f stylish --no-color",
    "stylish": "stylelint --syntax scss **/*.scss -o ./logs/stylelint.log",
    "fix": "npm run eslint-fix && npm run stylelint-fix",
    "eslint-fix": "eslint --fix . -o ./logs/eslint-fix.log -f stylish --no-color",
    "stylelint-fix": "stylelint --syntax scss --fix **/*.scss  -o ./logs/stylelint-fix.log",
    "clean": "rm -rf assets/**/*.css* && rm -rf assets/**/*.js* && rm -rf logs/*.log* && rm -rf logs/**/*.log*",
    "clean-logs": "rm -rf logs/*.log",
    "watch": "webpack --mode=development --env.NODE_ENV=local --env.MODE=development --watch --config webpack.config.js",
    "build": "webpack --mode=production  --env.MODE=production --config webpack.config.js"
  },
  "keywords": [
    "Issue Tracker",
    "Bug Report",
    "Feedback",
    "Bug Tracker"
  ],
  "author": "ThemeOO",
  "license": "GPL-3.0-or-later",
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.11.0",
    "@wordpress/eslint-plugin": "^4.1.0",
    "autoprefixer": "^9.8.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.6.0",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "eslint-plugin-prettier": "^3.1.4",
    "extract-loader": "^5.1.0",
    "file-loader": "^6.2.0",
    "husky": "^4.2.5",
    "jquery": "^3.5.1",
    "minimist": "^1.2.5",
    "pixrem": "^5.0.0",
    "postcss-loader": "^3.0.0",
    "prettier": "2.0.5",
    "sass": "^1.26.10",
    "sass-loader": "^8.0.2",
    "stylelint": "13.6.0",
    "stylelint-config-wordpress": "^17.0.0",
    "stylelint-scss": "^3.18.0",
    "terser-webpack-plugin": "^2.3.8",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  },
  "homepage": "https://themeoo.com/plugins/website-issue-tracker",
  "repository": {
    "type": "git",
    "url": "https://github.com/ThemeOO/website-issue-tracker"
  }
}
